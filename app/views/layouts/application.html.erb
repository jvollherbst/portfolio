<header>
  <h1 class="logo"><%= link_to "Jasmine Vollherbst", root_path %></h1>


  <nav>
    <ul>
      <li><%= link_to "About", about_index_path %></li>
  <% if session[:current_user_id] %>
  <li><%= link_to "Welcome #{User.find(session[:current_user_id]).username}",   user_path(session[:current_user_id]) %></li>
  <li><%= link_to "Create", new_image_path %></li>
  <li><%= link_to "Logout", sessions_path, method: :delete %></li>
  <% else %>

  <li><%= link_to "Login", users_path %></li>

  <% end %>
  </ul>
  </nav>
</header>

 <%= yield %>
<footer>
