<div class ="form">
  <h1>Create New Menu Items</h1>
    <%= form_for @image, html: { multipart: true } do |f| %>
    <%= f.label :imgname%><br>
    <%= f.text_field :imgname%><br>

    <%= f.label :category%><br>
    <%= f.text_field :category%><br>

    <%= f.label :tag%><br>
    <%= f.text_field :tag%><br>

    <%= f.label :description%><br>
    <%= f.text_field :description%><br>

    <%= f.label :imgurl%><br>
    <%= f.text_field :imgurl%><br>

    <br>
    <%= f.submit "Submit"%><br>
    <% end %>
</div>
